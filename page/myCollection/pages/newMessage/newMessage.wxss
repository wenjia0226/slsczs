/* page/myCollection//pages/newMessage/mewMessage.wxss */
.main {
  position: absolute;
  left: 0;
  right:0;
  top: 0;
  bottom: 0;
}
.itemWrap {
  margin: 10rpx 5%;
  height: 150rpx;
  display: flex;
}
.imgBox {
  width: 100rpx;
  height: 100rpx;
  border-radius: 10rpx;
}
.imgBox > image {
  width: 100%;
  height: 100%;
}
.rightBox {
  width: 140rpx;
  height: 140rpx;
  border-radius: 10rpx;
}
.centerBox {
  flex: 1;
  margin: 0 20rpx;
}
.title {
  font-size: 28rpx;
  color: cadetblue;
}
.img {
  margin: 10rpx 0;
  width: 30rpx;
  height: 30rpx;
}
.time {
  font-size: 24rpx;

}