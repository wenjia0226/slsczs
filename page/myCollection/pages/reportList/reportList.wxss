/* reportList/reportList.wxss */
.main {
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  display: flex;
  flex-direction: column;
  margin: 10rpx;
}
.itemWrap {
  display: flex;
  height: 180rpx;
  background: #4db2fd;
  border-radius: 20rpx;
  margin:10rpx;
}
.leftWrap {
  display: flex;
  justify-content: center;
  align-items: center;
  padding: 20rpx;
}
.logo {
  width:150rpx;
  height: 150rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.logo image {
  width:100%;
  height: 100%;
}
.centerWrap {
  color: #fff;
  flex: 1;
  display: flex;
  justify-content: center;
  flex-direction: column;
}
.name {
  font-size: 30rpx;
}
.time {
  font-size: 24rpx;
  margin: 10rpx 0
}
.rightWrap {
 font-size: 26rpx;
  color: #fff;
  display: flex;
  justify-content: center;
  align-items: center;

}
.rightGo {
  width: 40rpx;
  height: 40rpx;
}
.nodangan {
  text-align: center;
  background: #eee;
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
}
.no {
  /* background: #fff; */
  height: 200rpx;
  line-height: 200rpx;
  margin-top: 100rpx;
}