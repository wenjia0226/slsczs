/* answerResult/answerResult.wxss */
.mainWrap {
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  display: flex;
  align-items: center;
  justify-content: center
}
.titleWrap {
  margin: 50rpx 20rpx;
  border-radius: 50%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  width: 60%;
}
.titleImg {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  margin: 30rpx;
}
.nameWrap {
  margin: 40rpx 10rpx;
}
.name {
  font-weight: bold;
  font-size: 36rpx;
}
.centerWrap {
    font-weight: bold;
    font-size: 36rpx;
}
.conItem {
  padding: 10rpx;
  text-align: center;
}
.see {
  margin: 40rpx 0;
  padding: 20rpx 30rpx;
  background: #5083eb;
  color: #fff;
  border-radius: 40rpx;
  text-align: center;
  width: 100%;
}
.home {
  margin: 40rpx 0;
  padding: 20rpx 30rpx;
  color: #9e9e9e;
  border-radius: 40rpx;
  border: 2rpx solid #9e9e9e;
  text-align: center;
  width: 100%;
}
.getResult {
  padding: 10rpx 20rpx;
  background: red;
  border-radius: 30rpx;
}
 /* 恭喜页面 */
 .bg2 {
  display: block;
  position: absolute;
  top: 0%;
  left: 0%;
  width: 100%;
  height: 100%;
  background-color: black;
  z-index: 1001;
  -moz-opacity: 0.7;
  opacity: 0.70;
  filter: alpha(opacity=70);
}
 
.show {
  display: block;
  text-align: center;
  position: absolute;
  top: 20%;
  left: 10%;
  width: 80%;
  height: 60%;
  padding: 8rpx;
  box-sizing: border-box;
  background-color: white;
  z-index: 1002;
  overflow: auto;
  margin: auto;
  background-image: url('https://www.guangliangkongjian.com/images/bg_pop.png');
  background-position: center;
  background-repeat: no-repeat;
}
.innerBg {
  position: absolute;
  top: 20%;
  left: 10%;
  width: 80%;
  height: 60%;
}
.congratulaition {
  height: 200rpx;
  text-align: center;
}
.congratulaition > image {
  height: 180rpx;
  width: 90%;
  margin: 20rpx 0;
}
.biWrap {
  font-size: 40rpx;
  color: #fff;
}
.flowerWrap {
  height: 400rpx;
  position: relative;
}
.flower,.eye {
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  width: 100%;
  z-index: 99;
  height:400rpx;
}
.eye {
 z-index: 100;
  height: 300rpx;
}
.number {
  color: #FFDC02;
  font-size: 46rpx;
   font-weight: bold;
}
.buttonWrap {
  display: flex;
  justify-content: space-around
}
 .see {
  width: 200rpx;
  padding: 10rpx ;
  font-size: 36rpx;
  font-weight: bold;
  color: #A85326;
  background: #FFDC02;
  border-radius: 50rpx;
}
.seeMore {
  color: #FF9B00;
}