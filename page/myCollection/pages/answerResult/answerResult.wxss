/* answerResult/answerResult.wxss */
.mainWrap {
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  display: flex;
  align-items: center;
  justify-content: center
}
.titleWrap {
  margin: 50rpx 20rpx;
  border-radius: 50%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.titleImg {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  margin: 30rpx;
}
.nameWrap {
  margin: 40rpx 10rpx;
}
.name {
  font-weight: bold;
  font-size: 36rpx;
}
.centerWrap {
    font-weight: bold;
    font-size: 36rpx;
}
.conItem {
  padding: 10rpx;
}
.see {
  margin: 40rpx 0;
  width: 200rpx;
  padding: 20rpx 30rpx;
  background: #5083eb;
  color: #fff;
  border-radius: 40rpx;
  text-align: center;
}
.getResult {
  padding: 10rpx 20rpx;
  background: red;
  border-radius: 30rpx;
}