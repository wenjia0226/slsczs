<view  class="main">
    <swiper class="headerWrapper"  circular="{{true}}" current="0" previous-margin="20rpx" next-margin="20rpx"   bindchange="handleChange" wx:if="{{navList.length}}">
      <block wx:for="{{navList}}" wx:key="id" wx:if="{{navList}}">
        <swiper-item>
          <view class="swiperwrap">
            <view class="logo">
                <!-- 男孩及段位 -->
              <image src="https://www.guangliangkongjian.com/images/b1.png"  mode="aspectFit" wx:if="{{ item.gender == 0 && 300 >=item.myIntegral > 0}}"></image>
                <image src="https://www.guangliangkongjian.com/images/b2.png"  mode="aspectFit" wx:if="{{item.gender == 0 && 600 >=item.myIntegral > 300}}"></image>
                  <image src="https://www.guangliangkongjian.com/images/b3.png"  mode="aspectFit" wx:if="{{ item.gender == 0 && 900 >=item.myIntegral > 600}}"></image>
                    <image src="https://www.guangliangkongjian.com/images/b4.pngg"  mode="aspectFit" wx:if="{{ item.gender == 0 && 1200 >=item.myIntegral > 900}}"></image>
                      <image src="https://www.guangliangkongjian.com/images/b5.png"  mode="aspectFit" wx:if="{{item.gender == 0 && 1500 >=item.myIntegral > 1200}}"></image>
                        <image src="https://www.guangliangkongjian.com/images/b6.png"  mode="aspectFit" wx:if="{{ item.gender == 0 && 49999 >=item.myIntegral > 1500}}"></image>
                          <image src="https://www.guangliangkongjian.com/images/b7.png"  mode="aspectFit" wx:if="{{ item.gender == 0 &&item.myIntegral >= 50000}}"></image>
               <!-- 女孩及段位 -->
              <image src="https://www.guangliangkongjian.com/images/g1.png"  mode="aspectFit" wx:if="{{ item.gender == 1 && 300 >=item.myIntegral > 0}}"></image>
                <image src="https://www.guangliangkongjian.com/images/g2.png"  mode="aspectFit" wx:if="{{ item.gender == 1 && 600 >=item.myIntegral > 300}}"></image>
                  <image src="https://www.guangliangkongjian.com/images/g3.png"  mode="aspectFit" wx:if="{{ item.gender == 1 && 900 >=item.myIntegral > 600}}"></image>
                    <image src="https://www.guangliangkongjian.com/images/g4.pngg"  mode="aspectFit" wx:if="{{item.gender == 1 && 1200 >=item.myIntegral > 900}}"></image>
                      <image src="https://www.guangliangkongjian.com/images/g5.png"  mode="aspectFit" wx:if="{{ item.gender == 1 && 1500 >=item.myIntegral > 1200}}"></image>
                        <image src="https://www.guangliangkongjian.com/images/g6.png"  mode="aspectFit" wx:if="{{ item.gender == 1 && 49999 >=item.myIntegral > 1500}}"></image>
                          <image src="https://www.guangliangkongjian.com/images/g7.png"  mode="aspectFit" wx:if="{{item.gender == 1 &&item.myIntegral >= 50000}}"></image>
              <image style="width: 150rpx;height: 150rpx" wx:if="{{index == childrenList.length -1}}" src="/image/jia2.png"  mode="aspectFit" bindtap="gotoAddChild"></image>
            </view>
            <view class ="text">
                <view class="name">姓名：{{item.name}}</view>
                <view class="born">出生日期：{{item.birthday}}</view>
            </view>
          </view>
        </swiper-item>
      </block>
    </swiper>
     <view wx:if="{{!navList.length}}" class="headerWrapper nochild">
        <view>请先在筛查页添加或者选择孩子！！！</view>
    </view>
    <view  class="liebiao" wx:if="showList" >
      <view class=" title {{isSelect == 0 ? 'active': ''}}" data-type="0" bindtap="changeTab">裸眼检测</view>
      <view class="title {{isSelect == 1 ? 'active': ''}} " data-type="1" bindtap="changeTab"> 裸眼曲线</view>
       <view class=" title {{isSelect == 2 ? 'active': ''}}" data-type="2" bindtap="changeTab"> 戴镜检测</view>
      <view class="title {{isSelect == 3 ? 'active': ''}} " data-type="3" bindtap="changeTab">  戴镜曲线</view>
    </view>
    <!-- 裸眼检测列表 -->
    <swiper current="{{isSelect}}" bindchange="swiperChange" class="mainCon" wx:if="{{navList.length}}" >
      <block >
        <swiper-item style="margin-bottom: 30rpx;height: 100%" wx:if="{{dataList.length}}">
        <scroll-view scroll-y style="height: 100%">
         <view class="list" wx:for="{{dataList}}" wx:key="id">
          <view class="items" bindtap="gotArchiveDetail" data-id="{{item.id}}" data-type="luo">
            <view class="item" style="background: #6793ED; border-radius: 15rpx 0 0 15rpx">
              <view class="item-top" style="margin: 10rpx 0"><image mode="aspectFit" src="/image/eye.png"></image></view>
              <view class="genTime">{{item.date}}</view> 
              <view class="genTime">{{item.time}}</view> 
            </view>   
            <view class="item">
              <view  class="item-top"> <image  mode="aspectFit" src="/image/right.png"></image></view>
              <view>{{item.visionRight}}</view> 
            </view>
             <view class="item">
              <view  class="item-top"> <image  mode="aspectFit" src="/image/left.png"></image></view>
              <view>{{item.visionLeft}}</view> 
            </view>
          </view>
        </view>
        </scroll-view>
        </swiper-item>
        <swiper-item style="margin-bottom: 30rpx;height: 100%" wx:if="{{dataList.length == 0}}">       
            <view class="nodangan">暂无档案信息 {{dataList.length}}</view>
        </swiper-item>
      </block>
      <block >
        <swiper-item style="margin-bottom: 30rpx"  wx:if="{{picList.length}}">
          <view class="containter" wx:if="{{picList.length  != 0}}">
            <ec-canvas id="mychart-one" canvas-id="c" ec="{{ec}}" width="100%" height="100%"  ></ec-canvas>
          </view>
        </swiper-item>
        <swiper-item wx:if="{{picList.length == 0}}">
          <view class="nodangan">近7天无检测记录，请尽快检测</view>
        </swiper-item>
      </block>
      <!-- 戴镜 -->
      <block>
        <swiper-item wx:if="{{weardataList.length}}">
          <scroll-view scroll-y style="height: 100%" >
          <view class="list" wx:for="{{weardataList}}" wx:key="id">
            <view class="items" bindtap="gotArchiveDetail" data-id="{{item.id}}" data-type="wear">
              <view class="item" style="background: #6793ED; border-radius: 15rpx 0 0 15rpx">
                <view class="item-top" style="margin: 10rpx 0"><image mode="aspectFit" src="/image/eye.png"></image></view>
                <view class="genTime">{{item.date}}</view> 
                <view class="genTime">{{item.time}}</view>
              </view>
              <view class="item">
                <view  class="item-top"> <image  mode="aspectFit" src="/image/right.png"></image></view>
                <view>{{item.visionRight}}</view> 
              </view>
              <view class="item">
                <view  class="item-top"> <image  mode="aspectFit" src="/image/left.png"></image></view>
                <view>{{item.visionLeft}}</view> 
              </view>
            </view>
          </view>
          </scroll-view>
        </swiper-item>
        <swiper-item style="margin-bottom: 30rpx;height: 100%"  wx:if="{{weardataList.length == 0}}">
          <view class="nodangan">暂无档案信息</view>
        </swiper-item>
      </block>
      <block>
        <swiper-item style="margin-bottom: 30rpx" wx:if="{{wearpicList.length}}">
          <view class="containter" >
            <ec-canvas id="mychart-two" canvas-id="c" ec="{{ec2}}" width="100%" height="100%"></ec-canvas>
          </view>
        </swiper-item>
        <swiper-item wx:if="{{wearpicList.length == 0}}">
            <view class="nodangan">近7天无检测记录，请尽快检测</view>
        </swiper-item>
      </block>
    </swiper>
    <block wx:if="{{navList.length == 0}}">
     <view class="nodangan">请先在筛查页添加或者选择孩子！！！</view>
    </block>  
</view>