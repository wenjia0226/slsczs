/* pages/save/save.wxss */
/* 中间圆圈 */
.titlebar {
  background: #5853F0;
}
.bg {
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  z-index: -999;
  display: block;
  width: 100%;
}
.topWrap {
  background: #5853F0;
  text-align: center;
  position: fixed;
  width: 100%;
  top: 0;
  left: 0;
  height: 100rpx;
  line-height: 100rpx;
  z-index: 999;
}
.main {
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  display: flex;
  flex-direction: column;
}
.total {
  text-align: center;
  color: #fff;
  border-radius: 50rpx;
  font-size: 60rpx;
  font-weight: bold;
  letter-spacing: 20rpx;
  height: 100px;
  line-height: 100rpx;
}
/* //中间圆圈 */
.centerWrap{
  padding: 30rpx;
  display: flex;
}
.leftWrap {
  width: 250rpx;
  height: 250rpx;
  border-radius: 50%;
  border:15rpx solid #BFCEFE;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  box-shadow: 0rpx 0rpx  10rpx 10rpx #EFF3FF;
  margin: 30rpx;
}
.title {
  color: #999;
}
.bigNum {
  font-size: 46rpx;
  color: #4db2fd;
  margin: 20rpx;
  font-weight: bold;
}
.smallNum {
  font-size: 20rpx;
  color: #999;
}
.warning {
  margin: 5rpx 5%;
  font-size: 28rpx;
  line-height: 50rpx;
}
.end{
  margin: 20rpx auto;
  width: 64%;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  border-radius: 50rpx;
  color: #fff;
  font-size: 36rpx;
  letter-spacing: 3rpx;
}